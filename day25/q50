#include <stdio.h>

int main() {
    int i, j;
    int n = 5;  // Number of rows

    for(i = n; i >= 1; i--) {  // Loop for each row
        // Print leading spaces
        for(j = 0; j < n - i; j++) {
            printf(" ");
        }
        // Print stars
        for(j = 0; j < i; j++) {
            printf("*");
        }
        printf("\n");
    }

    return 0;
}
