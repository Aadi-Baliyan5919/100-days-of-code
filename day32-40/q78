#include <iostream>
using namespace std;

int main() {
    int n, sum = 0;
    cout << "Enter size of square matrix: ";
    cin >> n;

    int mat[n][n];
    cout << "Enter matrix elements:\n";
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> mat[i][j];

    for (int i = 0; i < n; i++)
        sum += mat[i][i];

    cout << "Sum of main diagonal elements = " << sum << endl;
    return 0;
}
