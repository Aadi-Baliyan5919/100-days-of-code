#include <iostream>
using namespace std;

int main() {
    int rows, cols, sum = 0;
    cout << "Enter number of rows and columns: ";
    cin >> rows >> cols;

    int mat[rows][cols];
    cout << "Enter matrix elements:\n";
    for (int i = 0; i < rows; i++)
        for (int j = 0; j < cols; j++) {
            cin >> mat[i][j];
            sum += mat[i][j];
        }

    cout << "Sum of all elements = " << sum << endl;
    return 0;
}
