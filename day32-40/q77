#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter size of square matrix: ";
    cin >> n;

    int mat[n][n];
    cout << "Enter matrix elements:\n";
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> mat[i][j];

    bool distinct = true;
    for (int i = 0; i < n && distinct; i++)
        for (int j = i + 1; j < n; j++)
            if (mat[i][i] == mat[j][j]) {
                distinct = false;
                break;
            }

    if (distinct)
        cout << "Diagonal elements are distinct.\n";
    else
        cout << "Diagonal elements are not distinct.\n";

    return 0;
}
