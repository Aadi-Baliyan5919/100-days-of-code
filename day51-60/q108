#include <iostream>
using namespace std;

int main(){
    int n;
    cout << "Enter size of array: ";
    cin >> n;
    int arr[n], ans[n];
    cout << "Enter array elements:\n";
    for(int i=0;i<n;i++) cin >> arr[i];

    int leftProd[n], rightProd[n];
    leftProd[0]=1;
    for(int i=1;i<n;i++) leftProd[i]=leftProd[i-1]*arr[i-1];

    rightProd[n-1]=1;
    for(int i=n-2;i>=0;i--) rightProd[i]=rightProd[i+1]*arr[i+1];

    for(int i=0;i<n;i++) ans[i]=leftProd[i]*rightProd[i];

    for(int i=0;i<n;i++){
        if(i>0) cout << " ";
        cout << ans[i];
    }
    cout << endl;
    return 0;
}
