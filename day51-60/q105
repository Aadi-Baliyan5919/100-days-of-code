#include <iostream>
using namespace std;

int main(){
    int n;
    cout << "Enter size of array: ";
    cin >> n;
    int arr[n];
    cout << "Enter array elements:\n";
    for(int i=0;i<n;i++) cin >> arr[i];

    // Boyer-Moore Majority Vote Algorithm
    int candidate=arr[0], count=1;
    for(int i=1;i<n;i++){
        if(arr[i]==candidate) count++;
        else{
            count--;
            if(count==0){
                candidate=arr[i];
                count=1;
            }
        }
    }

    // Verify
    count=0;
    for(int i=0;i<n;i++)
        if(arr[i]==candidate) count++;

    if(count>n/2) cout << candidate << endl;
    else cout << -1 << endl;

    return 0;
}
