#include <iostream>
using namespace std;

int main(){
    int n;
    cout << "Enter size of array: ";
    cin >> n;
    int arr[n], pge[n];
    cout << "Enter array elements:\n";
    for(int i=0;i<n;i++) cin >> arr[i];

    for(int i=0;i<n;i++){
        pge[i]=-1;
        for(int j=i-1;j>=0;j--){
            if(arr[j]>arr[i]){
                pge[i]=arr[j];
                break;
            }
        }
    }

    for(int i=0;i<n;i++){
        if(i>0) cout << ", ";
        cout << pge[i];
    }
    cout << endl;
    return 0;
}
