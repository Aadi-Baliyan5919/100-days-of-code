#include <iostream>
#include <climits>
using namespace std;

int main(){
    int n;
    cout << "Enter size of array: ";
    cin >> n;
    int arr[n];
    cout << "Enter array elements:\n";
    for(int i=0;i<n;i++) cin >> arr[i];

    int maxEnding=arr[0], maxSoFar=arr[0];
    for(int i=1;i<n;i++){
        maxEnding = max(arr[i], maxEnding+arr[i]);
        maxSoFar = max(maxSoFar, maxEnding);
    }

    cout << maxSoFar << endl;
    return 0;
}
