#include <iostream>
using namespace std;

int main() {
    int n, k;
    cout << "Enter size of array: ";
    cin >> n;
    int arr[n];
    cout << "Enter array elements:\n";
    for(int i=0;i<n;i++) cin >> arr[i];
    cout << "Enter window size k: ";
    cin >> k;

    for(int i=0;i<=n-k;i++) {
        int firstNeg = 0;
        for(int j=i;j<i+k;j++){
            if(arr[j]<0){
                firstNeg=arr[j];
                break;
            }
        }
        if(i>0) cout << " ";
        cout << firstNeg;
    }
    cout << endl;
    return 0;
}
