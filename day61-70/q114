#include <iostream>
#include <cstring>
using namespace std;

int main(){
    char str[100];
    cout << "Enter string: ";
    cin.getline(str,100);

    int n = strlen(str);
    int freq[256]={0}, start=0, maxLen=0;

    for(int end=0; end<n; end++){
        freq[str[end]]++;
        while(freq[str[end]]>1){
            freq[str[start]]--;
            start++;
        }
        if(end-start+1 > maxLen) maxLen = end-start+1;
    }

    cout << maxLen << endl;
    return 0;
}
