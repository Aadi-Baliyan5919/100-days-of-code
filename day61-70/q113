#include <iostream>
#include <algorithm>
using namespace std;

int main(){
    int n,k;
    cout << "Enter size of array: ";
    cin >> n;
    int arr[n];
    cout << "Enter array elements:\n";
    for(int i=0;i<n;i++) cin >> arr[i];
    cout << "Enter k: ";
    cin >> k;

    sort(arr, arr+n);
    cout << arr[k-1] << endl; // 1-based k
    return 0;
}
