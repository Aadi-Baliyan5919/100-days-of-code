#include <iostream>
using namespace std;

int main(){
    int m,n;
    cout << "Enter size of first array: ";
    cin >> m;
    int arr1[m];
    cout << "Enter first sorted array:\n";
    for(int i=0;i<m;i++) cin >> arr1[i];

    cout << "Enter size of second array: ";
    cin >> n;
    int arr2[n];
    cout << "Enter second sorted array:\n";
    for(int i=0;i<n;i++) cin >> arr2[i];

    int i=0,j=0;
    while(i<m && j<n){
        if(arr1[i]<=arr2[j]) cout << arr1[i++] << " ";
        else cout << arr2[j++] << " ";
    }
    while(i<m) cout << arr1[i++] << " ";
    while(j<n) cout << arr2[j++] << " ";
    cout << endl;
    return 0;
}
