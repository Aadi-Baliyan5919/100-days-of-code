#include <iostream>
using namespace std;

int main(){
    int n;
    cout << "Enter size of array: ";
    cin >> n;
    int arr[n];
    cout << "Enter array elements:\n";
    for(int i=0;i<n;i++) cin >> arr[i];

    int repeated=-1;
    for(int i=0;i<n;i++){
        int idx=abs(arr[i]);
        if(arr[idx]<0) { repeated=idx; break;}
        arr[idx] = -arr[idx];
    }

    cout << repeated << endl;
    return 0;
}
