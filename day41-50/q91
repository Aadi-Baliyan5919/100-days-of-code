#include <iostream>
using namespace std;

int main() {
    char str[100], result[100];
    cout << "Enter a string: ";
    cin.getline(str, 100);

    int j = 0;
    for (int i = 0; str[i] != '\0'; i++) {
        char ch = str[i];
        ch = tolower(ch);
        if (ch!='a' && ch!='e' && ch!='i' && ch!='o' && ch!='u') {
            result[j++] = str[i];
        }
    }
    result[j] = '\0';

    cout << "String after removing vowels: " << result << endl;
    return 0;
}
