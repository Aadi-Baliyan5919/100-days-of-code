#include <iostream>
using namespace std;

int main() {
    char str[100];
    cout << "Enter a string: ";
    cin.getline(str,100);

    for(int i=0; str[i]!='\0'; i++) {
        for(int j=i; str[j]!='\0'; j++) {
            for(int k=i;k<=j;k++) cout << str[k];
            cout << endl;
        }
    }

    return 0;
}
