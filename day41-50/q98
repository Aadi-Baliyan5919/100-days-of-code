#include <iostream>
#include <cstring>
using namespace std;

int main() {
    char str[100];
    cout << "Enter full name: ";
    cin.getline(str,100);

    int n = strlen(str);
    int lastSpace = -1;
    for(int i=n-1;i>=0;i--) {
        if(str[i]==' ') {
            lastSpace = i;
            break;
        }
    }

    // Print initials of all except last word
    int i=0;
    while(i<lastSpace) {
        if(i==0 || (str[i-1]==' ' && str[i]!=' ')) cout << (char)toupper(str[i]) << ".";
        i++;
    }

    // Print surname
    for(int j=lastSpace+1;j<n;j++) cout << str[j];
    cout << endl;
    return 0;
}
